<view><view class="container"><view class="item"><view data-event-opts="{{[['tap',[['changeAvatar',['$event']]]]]}}" class="avatar" catchtap="__e"><text class="aside-left">头像</text><image class="image_radius" src="{{userInfo.avatar||'https://sdk-web-1252463788.cos.ap-hongkong.myqcloud.com/component/TUIKit/assets/avatar_21.png'}}"></image><image class="listimage" src="/static/static/assets/detail.svg"></image></view></view><view class="item"><view data-event-opts="{{[['tap',[['handleEditToggle',['$event']]]]]}}" class="nickname" bindtap="__e"><text class="aside-left">昵称</text><text class="aside-right">{{userInfo.nick?userInfo.nick:'提莫'}}</text><image class="listimage" src="/static/static/assets/detail.svg"></image></view></view></view><block wx:if="{{popupToggle}}"><view data-event-opts="{{[['tap',[['handleEditToggle',['$event']]]]]}}" class="popup-mask" catchtap="__e"><view data-event-opts="{{[['tap',[['handleCatchTap',['$event']]]]]}}" class="popup-main" catchtap="__e"><view class="popup-main-header"><label class="popup-main-header-title">修改昵称</label><button class="button" disabled="{{!infoNick}}" data-event-opts="{{[['tap',[['handleEditSubmit',['$event']]]]]}}" catchtap="__e">确认</button></view><input class="popup-main-input" type="text" data-event-opts="{{[['input',[['bindEditInput',['$event']]]]]}}" value="{{infoNick}}" bindinput="__e"/><label class="text">仅限中文、字母、数字和下划线，2-20个字</label></view></view></block><block wx:if="{{popupToggleAvatar}}"><view data-event-opts="{{[['tap',[['handleEditToggleAvatar',['$event']]]]]}}" class="popup-mask-avatar" catchtap="__e"><view data-event-opts="{{[['tap',[['handleCatchTap',['$event']]]]]}}" class="popup-main-avatar" catchtap="__e"><view class="pop-main-header-avatar" data-item="{{item}}"><label class="popup-main-header-title-avatar">设置头像</label><label data-event-opts="{{[['tap',[['handleEditSubmitAvatar',['$event']]]]]}}" class="button-avatar" catchtap="__e">确认</label><view class="image-list"><block wx:for="{{avatarList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><image class="{{['image-avatar '+(item.URL===avatar&&'image-avatar-active')]}}" src="{{item.URL}}" data-value="{{item}}" data-event-opts="{{[['tap',[['click',['$event']]]]]}}" bindtap="__e"></image></block></view></view></view></view></block></view>